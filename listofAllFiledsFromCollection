#@author narumeena
#@description get a list of all fields from a collection 



mr = db.runCommand({
  "mapreduce" : "things",
  "map" : function() {
    for (var key in this) { emit(key, null); }
  },
  "reduce" : function(key, stuff) { return null; },
  "out": "things" + "_keys"
})

db[mr.result].distinct("_id")